{
  "name": "todo-mvc-refactor-stencil-web-components",
  "version": "1.0.0",
  "description": "todo-mvc-refactor-stencil-web-components",
  "scripts": {
    "bootstrap": "run-p install:*",
    "install:todo-mvc-apps": "cd todo-mvc-apps && npm install",
    "install:todo-mvc-stencil-web-components": "cd todo-mvc-stencil-web-components && npm install",
    "install:todo-mvc-cypress-tests": "cd todo-mvc-cypress-tests && npm install",
    "deploy": "run-p deploy:*",
    "deploy:todo-mvc-apps": "cd todo-mvc-apps && npm run deploy",
    "deploy:todo-mvc-stencil-web-components": "cd todo-mvc-stencil-web-components && npm run deploy",
    "start": "run-p start:*",
    "start:todo-mvc-apps": "cd todo-mvc-apps && npm start",
    "start:todo-mvc-stencil-web-components": "cd todo-mvc-stencil-web-components && npm start",
    "xstart:todo-mvc-cypress-tests": "cd todo-mvc-cypress-tests && npm start"
  },
  "keywords": [],
